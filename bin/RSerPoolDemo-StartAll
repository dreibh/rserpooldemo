#!/bin/bash

./ComponentControl ./RSerPoolDemo-PR.config 1 start -identifier=0x10000
./ComponentControl ./RSerPoolDemo-PR.config 2 start -identifier=0x10001
sleep 5
./ComponentControl ./RSerPoolDemo-PE.config 1 start -identifier=0x100
./ComponentControl ./RSerPoolDemo-PE.config 2 start -identifier=0x101
./ComponentControl ./RSerPoolDemo-PE.config 3 start -identifier=0x102
sleep 2
./ComponentControl ./RSerPoolDemo-PU.config 1 start -identifier=0x1 -caption=PoolUser-1
sleep 1
./ComponentControl ./RSerPoolDemo-PU.config 2 start -identifier=0x2 -caption=PoolUser-2
